{
  "name": "turtle",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build:css && wrangler dev",
    "deploy": "npm run build:css && wrangler deploy --minify src/index.tsx",
    "build": "npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./public/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/styles/input.css -o ./public/styles.css --watch"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "hono": "^4.6.14"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "autoprefixer": "^10.4.20",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "wrangler": "^3.87.0"
  }
}
